public class MaxElementInArray {
    public static void main(String[] args) {
        int[] arr = {5, 8, 3, 12, 9, 6};
        int max = findMax(arr);
        System.out.println("Maximum element in the array: " + max);
    }

    public static int findMax(int[] arr) {
        int max = arr[0];
        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }
        return max;
    }
}
